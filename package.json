{
    "name": "@codegouvfr/rescript-react-dsfr",
    "description": "ReScript bindings for react-dsfr",
    "version": "0.1.1",
    "scripts": {
        "clean": "rescript clean",
        "build": "rescript build -with-deps",
        "watch": "rescript build -w -with-deps",
        "demo": "yarn --cwd demo dev",
        "generate-classNames": "bun scripts/generateClassNames.ts",
        "postinstall": "yarn run generate-classNames"
    },
    "keywords": [
        "rescript",
        "dsfr",
        "rescript-bindings",
        "react-dsfr",
        "design-system"
    ],
    "author": "Emile Rolley <emile.rolley@tuta.io>",
    "license": "MIT",
    "dependencies": {
        "@codegouvfr/react-dsfr": "^1.2.1",
        "@rescript/react": "^0.11.0",
        "react": "^18.2.0"
    },
    "devDependencies": {
        "@types/bun": "^1.0.3",
        "rescript": "^10.1.4"
    },
    "publishConfig": {
        "access": "public"
    }
}
