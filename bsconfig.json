{
    "name": "@codegouvfr/rescript-react-dsfr",
    "namespace": "ReactDSFR",
    "version": "0.1.0",
    "sources": [
        {
            "dir": "src",
            "subdirs": true
        }
    ],
    "package-specs": [
        {
            "module": "es6",
            "in-source": true
        }
    ],
    "suffix": ".bs.js",
    "refmt": 3,
    "jsx": { "version": 4, "mode": "classic" },
    "bs-dependencies": ["@rescript/react"],
    "gentypeconfig": {
        "language": "typescript",
        "shims": {},
        "generatedFileExtension": ".gen.tsx",
        "module": "es6",
        "debug": {
            "all": false,
            "basic": false
        }
    }
}
